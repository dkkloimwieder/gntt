{
    "name": "ganttss",
    "version": "1.0.3",
    "description": "",
    "main": "dist/ganttss.umd.js",
    "module": "dist/ganttss.es.js",
    "type": "module",
    "scripts": {
        "dev": "vite --config config/vite/solid.js",
        "build": "vite build --config config/vite/solid.js",
        "build:demo": "vite build --config config/vite/demo.js",
        "preview:demo": "vite preview --config config/vite/demo.js --port 4173",
        "generate:calendar": "node src/scripts/generateCalendar.js",
        "generate:topology": "node src/scripts/generateTopology.js",
        "bench": "node benchmarks/constraint/constraintBench.js",
        "bench:scaling": "node benchmarks/constraint/scalingTest.js",
        "typecheck": "tsc --noEmit",
        "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
        "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\" \"tests/**/*.js\"",
        "prettier-check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\" \"tests/**/*.js\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dkkloimwieder/gntt.git"
    },
    "files": [
        "src",
        "dist",
        "README.md"
    ],
    "exports": {
        ".": {
            "require": "./dist/ganttss.umd.js",
            "import": "./dist/ganttss.es.js",
            "style": "./dist/ganttss.css"
        }
    },
    "keywords": [
        "gantt",
        "svg",
        "simple gantt",
        "project timeline",
        "interactive gantt",
        "project management"
    ],
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": ""
    },
    "homepage": "",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^8.51.0",
        "@typescript-eslint/parser": "^8.51.0",
        "babel-preset-solid": "^1.9.10",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^2.10.0",
        "eslint-plugin-prettier": "^2.7.0",
        "postcss-nesting": "^12.1.5",
        "prettier": "3.2.5",
        "typescript": "^5.9.3",
        "vite": "^7.3.0",
        "vite-plugin-solid": "^2.11.10"
    },
    "eslintIgnore": [
        "dist"
    ],
    "sideEffects": [
        "*.css"
    ],
    "packageManager": "pnpm@10.23.0+sha512.21c4e5698002ade97e4efe8b8b4a89a8de3c85a37919f957e7a0f30f38fbc5bbdd05980ffe29179b2fb6e6e691242e098d945d1601772cad0fef5fb6411e2a4b",
    "dependencies": {
        "@solid-primitives/memo": "^1.4.3",
        "@solid-primitives/raf": "^2.3.2",
        "@solid-primitives/resize-observer": "^2.1.3",
        "@solid-primitives/scheduled": "^1.5.2",
        "@solid-primitives/scroll": "^2.1.3",
        "solid-js": "^1.9.10"
    }
}
